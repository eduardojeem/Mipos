    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.8.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
