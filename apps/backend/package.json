{
  "name": "@pos-system/backend",
  "version": "1.0.0",
  "description": "Backend API para sistema POS",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index-minimal.ts",
    "dev:full": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "prisma migrate dev",
